<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="all_star.all_star_chat"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="all_star.all_star_chat.Login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="all_star.all_star_chat.Chat"
            android:label="@string/title_activity_chat"
            android:parentActivityName="all_star.all_star_chat.Login" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="all_star.all_star_chat.Login" />
        </activity>
    </application>

</manifest>